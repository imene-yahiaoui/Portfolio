{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-mask-image",
          "-webkit-text-size-adjust",
          "zoom",
          "text-align-last",
          "backface-visibility"
        ]
      }
    ],
    "apple-touch-icons": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 111",
    "not chrome <= 110",
    "not edge <= 110",
    "not firefox <= 110",
    "not safari <= 16.3",
    "not samsung <= 19"
  ]
}